{
  "name": "turborepo-nuxt-boilerplate",
  "version": "0.0.5",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@myturborepo/eslint-config-custom": "workspace:*",
    "@release-it/conventional-changelog": "^8.0.1",
    "release-it": "^16.3.0",
    "turbo": "^1.10.16"
  },
  "engines": {
    "node": ">=v18.12.1"
  },
  "packageManager": "pnpm@7.18.1"
}
